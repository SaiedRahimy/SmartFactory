{
  "UseInMemoryDatabase": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SmartFactorySample.DataReceptionitectureDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "MessageQueueProvider": "Kafka",
  "ConsumerKafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "Sensor-topic",
    "GroupId": "group1"
  },
  "ConsumerRabbitMq": {
    "HostName": "localhost",
    "QueueName": "Sensor-queue"
  },

  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "Archive-topic",
    "GroupId": "group1"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "QueueName": "Archive-queue"
  },
  "IdentityServerUrl": "https://localhost:5001"

}
