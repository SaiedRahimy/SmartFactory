{
  "UseInMemoryDatabase": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SmartFactorySample.DataPresentationitectureDb;Trusted_Connection=True;MultipleActiveResultSets=true;",
    "InfluxDB": "http://your-influxdb:8086"

  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "MessageQueueProvider": "Kafka",
  "ConsumerKafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "Archive-topic",
    "GroupId": "group1"
  },
  "ConsumerRabbitMq": {
    "HostName": "localhost",
    "QueueName": "Archive-queue"
  },
  "SyncRedisPeriod": 15, //Minutes
  "RedisUrl": "localhost:6379",
  "InfluxDbToken": "Your-InfluxDB-Token",
  "InfluxDbOrg": "Your-Organization",
  "BucketDaily": "sensor_daily",
  "BucketMonthly": "sensor_monthly",
  "BucketYearly": "sensor_yearly"

}
